<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	  <IncludeBuiltProjectOutputGroup>true</IncludeBuiltProjectOutputGroup>
	  <Description>Simple Config library that allows changing config properties at runtime, as well as Serialization and Deserialization.</Description>
	  <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  <Authors>mortuus</Authors>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <GenerateDocumentationFile>True</GenerateDocumentationFile>
	  <Version>1.0.0</Version>
	  <PackageProjectUrl>https://github.com/mortuusars/Mortuus.Config</PackageProjectUrl>
	  <RepositoryUrl>https://github.com/mortuusars/Mortuus.Config</RepositoryUrl>
	  <RepositoryType>git</RepositoryType>
	  <PackageTags>csharp config settings configuration</PackageTags>
	  <PackageReleaseNotes>1.0.0 - Initial Release</PackageReleaseNotes>
  </PropertyGroup>

</Project>
